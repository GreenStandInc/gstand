{
  "name": "gstand",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.tsx",
    "build": "tsup --format esm",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/pg": "^8.11.10",
    "pino-pretty": "^11.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@atproto/api": "^0.13.14",
    "@atproto/oauth-client-node": "^0.2.0",
    "@hono/node-server": "^1.13.5",
    "better-sqlite3": "^11.5.0",
    "hono": "^4.6.9",
    "iron-session": "^8.0.3",
    "kysely": "^0.27.4",
    "pg": "^8.13.1"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
