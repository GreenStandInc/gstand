{
  "name": "gstand",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "ui:dev": "vite",
    "ui:build": "vite build",
    "dev": "tsx watch --clear-screen=false src/index.tsx",
    "build": "tsup",
    "start": "node dist/index.esm.js",
    "lexgen": "lex gen-server ./src/lexicon ./lexicons/* --yes && lex gen-api ./client/lexicon ./lexicons/* --yes",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@atproto/lex-cli": "^0.5.2",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "autoprefixer": "^10.4.20",
    "jest": "^29.7.0",
    "pino-pretty": "^11.3.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "@atproto/api": "^0.13.14",
    "@atproto/identity": "^0.4.3",
    "@atproto/lexicon": "^0.4.3",
    "@atproto/oauth-client-node": "^0.2.0",
    "@atproto/sync": "^0.1.6",
    "@atproto/xrpc-server": "^0.7.3",
    "@hono/node-server": "^1.13.5",
    "@hono/zod-validator": "^0.4.1",
    "better-sqlite3": "^11.5.0",
    "dotenv": "^16.4.5",
    "hono": "^4.6.9",
    "iron-session": "^8.0.3",
    "kysely": "^0.27.4",
    "multiformats": "^13.3.1",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
