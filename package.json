{
  "name": "gstand",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "tsup",
    "start": "node dist/index.cjs"
  },
  "devDependencies": {
    "pino-pretty": "^11.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@atproto/api": "^0.13.14",
    "@atproto/oauth-client-node": "^0.2.0",
    "@fastify/formbody": "^8.0.1",
    "@fastify/multipart": "^9.0.1",
    "fastify": "^5.1.0",
    "iron-session": "^8.0.3",
    "kysely": "^0.27.4"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
